<Window x:Class="RocketModder.InputWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RocketModder"
        mc:Ignorable="d"
        Title="InputWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <TextBox Grid.Row="0" x:Name="InputTb" Text="0"/>
        <DockPanel Grid.Row="1" LastChildFill="False">
            <Button Width="80" Content="OK" IsDefault="True" DockPanel.Dock="Right"/>
            <Button Width="80" Content="Cancel" IsDefault="True" DockPanel.Dock="Right"/>
        </DockPanel>
    </Grid>
</Window>
